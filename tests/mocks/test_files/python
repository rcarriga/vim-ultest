from unittest import TestCase


def test_a():

    x = 3


class TestAgain(TestCase):
    def test_d(self):  # type: ignore
        class MyClass:
            ...

        assert 3 == 3

    def test_a(self):  # type: ignore
        class MyClass:
            def test_a(self):  # type: ignore
                class MyClass:
                    ...

        assert 33 == 3


class TestMyClass(TestCase):
    def test_d(self):
        assert 33 == 3

    def test_a(self):
        assert 33 == 3


def test_b(self):
    assert 33 == 3

